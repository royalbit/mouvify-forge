{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD013": false,
  "MD022": true,
  "MD024": {
    "siblings_only": true
  },
  "MD025": true,
  "MD026": {
    "punctuation": ".,;:!"
  },
  "MD029": false,
  "MD032": true,
  "MD033": {
    "allowed_elements": [
      "br",
      "details",
      "summary",
      "sub",
      "sup"
    ]
  },
  "MD034": true,
  "MD040": true,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  },
  "MD051": false,
  "MD060": false,
  "MD007": false,
  "MD034": false,
  "MD012": false,
  "MD049": false
}
